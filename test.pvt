// A comment

// An implicit int.
log(2);
// An explicit int.
log(5i);

// An implicit float.
log(2.5);
// An explicit float.
log(3f);

// Booleans and multiple arguments.
log(true, false);

// Strings.
log("Hello world!");
// A string showing how different delimiters layer.
log("What's that over there?");
